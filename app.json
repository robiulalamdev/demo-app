{
  "expo": {
    "name": "Royalle",
    "slug": "ndemo",
    "version": "1.0.0",
    "plugins": [
      "expo-router",
      "@react-native-google-signin/google-signin",
      [
        "react-native-fbsdk-next",
        {
          "appID": "941161364444352",
          "clientToken": "27a35c6fcbfa13f6e30a1384c7438d3d",
          "displayName": "ndemo"
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/lato/Lato-Regular.ttf",
            "./assets/fonts/lato/Lato-SemiBold.ttf",
            "./assets/fonts/lato/Lato-Bold.ttf",
            "./assets/fonts/lato/Lato-Medium.ttf",
            "./assets/fonts/archivo/Archivo-Regular.ttf",
            "./assets/fonts/inter/Inter-Regular.otf",
            "./assets/fonts/inter/Inter-Medium.otf",
            "./assets/fonts/inter/Inter-SemiBold.otf",
            "./assets/fonts/inter/Inter-Bold.otf",
            "./assets/fonts/poppins/Poppins-Regular.ttf",
            "./assets/fonts/poppins/Poppins-Medium.ttf",
            "./assets/fonts/poppins/Poppins-SemiBold.ttf",
            "./assets/fonts/poppins/Poppins-Bold.ttf"
          ]
        }
      ]
    ],
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "ndemo",
    "userInterfaceStyle": "automatic",
    "main": "expo-router/entry",
    "splash": {
      "image": "./assets/brand/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#010404"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nahid.demo",
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "SKAdNetworkItems": [
          {
            "SKAdNetworkIdentifier": "v9wttpbfk9.skadnetwork"
          },
          {
            "SKAdNetworkIdentifier": "n38lu8286q.skadnetwork"
          }
        ]
      },
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/brand/splash.png",
        "backgroundColor": "#010404"
      },
      "googleServicesFile": "./google-services.json",
      "package": "com.nahid.demo",
      "permissions": [
        "android.permission.INTERNET"
      ],
      "runtimeVersion": "1.0.0"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c3a97332-02f0-48ad-b402-ac2b9d5170bc"
      }
    },
    "owner": "robiulalamdx",
    "updates": {
