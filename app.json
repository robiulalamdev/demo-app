{
  "expo": {
    "name": "ndemo",
    "slug": "ndemo",
    "version": "1.0.0",
    "plugins": [
      "expo-router",
      "@react-native-google-signin/google-signin",
      [
        "react-native-fbsdk-next",
        {
          "appID": "941161364444352",
          "clientToken": "27a35c6fcbfa13f6e30a1384c7438d3d",
          "displayName": "ndemo"
        }
      ]
    ],
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "ndemo",
    "userInterfaceStyle": "automatic",
    "main": "expo-router/entry",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nahid.demo",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "googleServicesFile": "./google-services.json",
      "package": "com.nahid.demo"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c3a97332-02f0-48ad-b402-ac2b9d5170bc"
      }
    },
    "owner": "robiulalamdx"
  }
}